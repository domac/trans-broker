{
    "index_patterns": "logs-endpoint-transbroker-*",
    "settings": {
        "index.refresh_interval": "5s"
    },
    "mappings": {
        "doc": {
            "dynamic_templates": [
                {
                    "strings": {
                        "match_mapping_type": "string",
                        "mapping": {
                            "type": "text",
                            "norms": false,
                            "fields": {
                                "raw": {
                                    "type": "keyword",
                                    "ignore_above": 256
                                }
                            }
                        }
                    }
                }
            ],
            "properties": {
                "@timestamp": {
                    "type": "date"
                },
                "@date_operation": {
                    "type": "date"
                },
                "event_time": {
                    "type": "date"
                },
                "dst_port": {
                    "type": "integer"
                },
                "src_port": {
                    "type": "integer"
                },
                "event_id": {
                    "type": "integer"
                },
                "process_id": {
                    "type": "integer"
                },
                "process_parent_id": {
                    "type": "integer"
                },
                "target_process_id": {
                    "type": "integer"
                },
                "version": {
                    "type": "integer"
                }
            }
        }
    }
}